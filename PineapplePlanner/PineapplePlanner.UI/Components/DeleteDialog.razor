<MudDialog>
    <DialogContent>
        <MudText>@ContentText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick=@HandleCancel>@CancelText</MudButton>
        <MudButton Color=@Color.Error Variant="Variant.Filled" OnClick=@HandleSubmit>@SubmitText</MudButton>
    </DialogActions>
</MudDialog>